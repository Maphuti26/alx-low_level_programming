#include "main.h"

/**
*_strcpy - copies characters
*@dest: where content is copied
*@src: string to be copied
*@n: characters to be copied
*
*Return: dest
*/

char *_strncpy(char *dest, char *src, int n);
{
	int j;

	j = 0;

	while (j < n && src[j] != '\0')
	{
		dest[j] = src[j];
		j++;
	}

	while (j < n)
	{
		dest[j] = '\0';
		j++;
	}

	return (dest);
}
