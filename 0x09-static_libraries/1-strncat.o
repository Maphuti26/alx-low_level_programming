#include "main.h"
/**
*_strncat - appends the pointed string to
*using the bytes from the src
*@dest: pointer to the destination array
*@src: string to be appended
*@n: number of characters to be appended
*
*Return: string dest
*/
char *_strncat(char *dest, char *src, int n);
{
	int i;
	int j;

	i = 0;
	while (dest[i] != '\0')
	{
		i++;
	}
	j = 0;
	while (j < n && src[j] != '\0')
	{
	dest[i] = src[j];
	i++;
	j++;
	}
	dest[i] = '\0';
	return (dest);
}
