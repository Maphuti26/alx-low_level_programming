#include "main.h"

/**
*_memcpy - copies characters from the memory area
*@dest: memory storage
*@src: copied memory
*@n: number of bytes to be copied
*
*Return: copied memory with the n byte changed
*/

char *_memcpy(char *dest, char *src, unsigned int n);
{
	int r = 0;
	int i = n;

	for (; r < i; r++)
	{
		dest[r] = src[r];
		n--;
	}
	return (dest);
}
