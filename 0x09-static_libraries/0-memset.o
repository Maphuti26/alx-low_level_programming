#inclide "main.h"

/**
*_memset - value of the memory to be filled
*@s: memory from start to be filled
*@b: value desired
*@n: bytes to be changed
*Return: new value for n bytes and change array
*/

char *_memset(char *s, char b, unsigned int n);
{
	int i = 0;


	for (; n > 0; i++)
	{
		s[i] = b;
		n--;
	}
	return (s);
}
